@Article{Roy-Camille1980,
  author        = {Roy-Camille, R. and Saillant, G. and Judet, T. and de Botton, G. and Michel, G.},
  title         = {{[{F}actors of severity in the fractures of the odontoid process (author's transl)]}},
  journal       = {Rev Chir Orthop Reparatrice Appar Mot},
