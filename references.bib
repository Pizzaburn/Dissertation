  issn-linking    = {0008-428X},
  issue           = {6},
  keywords        = {Atlanto-Occipital Joint, diagnostic imaging, injuries; Humans; Joint Dislocations, diagnosis, diagnostic imaging, mortality, therapy; Occipital Bone, injuries; Radiography; Skull Fractures, complications, diagnosis; Survival Rate},
  nlm             = {PMC3692674},
  nlm-id          = {0372715},
  owner           = {NLM},
  pmc             = {PMC3692674},
  pmid            = {11764873},
  pubmodel        = {Print},
