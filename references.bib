}

@Article{Oh2010,
  author   = {Oh, J. Y. and Chough, C. K. and Cho, C. B. and Park, H. K.},
  title    = {{{T}raumatic atlantoaxial rotatory fixation with accompanying odontoid and c2 articular facet fracture}},
