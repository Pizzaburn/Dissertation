  issn-linking    = {0361-803X},
  issue           = {1},
  keywords        = {Adult; Atlanto-Axial Joint, diagnostic imaging, injuries; Atlanto-Occipital Joint, diagnostic imaging, injuries; Female; Humans; Male; Middle Aged; Prognosis; Skull Base, diagnostic imaging, injuries; Tomography, X-Ray Computed, methods; Trauma, Nervous System, diagnostic imaging},
