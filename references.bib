@Article{Moreau2012,
  author   = {Moreau, P. E. and Nguyen, V. and Atallah, A. and Kassab, G. and Thiong'o, M. W. and Laporte, C.},
  title    = {{{T}raumatic atlantoaxial dislocation with odontoid fracture: {A} case report}},
  journal  = {Orthop Traumatol Surg Res},
  year     = {2012},
  volume   = {98},
  number   = {5},
  pages    = {613--617},
  month    = {Sep},
  note     = {[DOI:\href{https://dx.doi.org/10.1016/j.otsr.2012.03.012}{10.1016/j.otsr.2012.03.012}] [PubMed:\href{https://www.ncbi.nlm.nih.gov/pubmed/22901523}{22901523}]},
  abstract = {We are reporting the one-year follow-up for a case of traumatic atlantoaxial dislocation associated with an odontoid fracture. This injury combination is rare and serious because of its resulting instability. After an unsuccessful attempt at closed reduction with traction, an open reduction with occipitocervical fixation was performed using a posterior approach. Based on our experience and a review of the published literature, the method for managing such an injury is discussed. If closed reduction with traction is successful, subsequent treatment is based on the algorithms for isolated odontoid fractures. If the closed reduction fails, surgical treatment consists of an open reduction using a posterior or lateral retropharyngeal approach, and then fixation of C1-C2, which is the key procedure.},
  file     = {:Paper/Traumatic atlantoaxial dislocation with odontoid fracture\: A case report.pdf:PDF},
}

@Article{Meng2014,
  author   = {Meng, H. and Gao, Y. and Li, M. and Luo, Z. and Du, J.},
  title    = {{{P}osterior atlantoaxial dislocation complicating odontoid fracture without neurologic deficit: a case report and review of the literature}},
