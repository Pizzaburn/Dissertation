  keywords        = {Axis, Cervical Vertebra, diagnostic imaging, injuries; Cervical Vertebrae, diagnostic imaging; Humans; Radiography; Spondylolisthesis, diagnostic imaging, etiology, history},
  nlm-id          = {0075674},
